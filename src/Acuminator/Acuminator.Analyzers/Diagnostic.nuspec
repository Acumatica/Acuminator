<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Acuminator.Analyzers</id>
    <version>1.0.0.0</version>
    <title>Acuminator.Analyzers</title>
	<summary>Code Analyzers for Acumatica Framework</summary>
    <authors>Acumatica</authors>
    <owners>Acumatica</owners>
    <licenseUrl>https://github.com/Acumatica/Acuminator/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Acumatica/Acuminator</projectUrl>
    <icon>packageIcon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Code analyzers that help to find and fix common developer issues related to Acumatica Framework</description>
    <releaseNotes>https://github.com/Acumatica/Acuminator/blob/master/docs/ReleaseNotes.md</releaseNotes>
    <copyright>Acumatica (c) 2019</copyright>
    <tags>Acumatica, Acuminator, roslyn</tags>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
	<file src="packageIcon.png" target="" />
  </files>
</package>